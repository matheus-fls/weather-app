{
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": { "ecmaVersion": 6 },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-shadow": 0,
    "no-param-reassign": 0,
    "eol-last": 0,
    "arrow-parens": ["error", "always"],
    "import/no-cycle": "off"
  }
}
